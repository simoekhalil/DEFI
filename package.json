{
  "name": "web3-testing-app",
  "version": "1.0.0",
  "description": "TypeScript React app for testing Gala DeFi website with Playwright and Dappwright",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:gala": "node run-gala-launchpad-tests.js",
    "test:gala:critical": "node run-gala-launchpad-tests.js --suite=critical",
    "test:gala:features": "node run-gala-launchpad-tests.js --suite=features",
    "test:gala:integration": "node run-gala-launchpad-tests.js --suite=integration",
    "test:gala:mathematical": "node run-gala-launchpad-tests.js --suite=mathematical",
    "test:gala:graduation": "node run-gala-launchpad-tests.js --suite=graduation",
    "test:gala:performance": "node run-gala-launchpad-tests.js --suite=performance",
    "test:gala:headed": "node run-gala-launchpad-tests.js --headed",
    "test:gala:install": "node run-gala-launchpad-tests.js --install --suite=all",
    "test:graduation": "playwright test tests/graduation-rewards.spec.ts tests/graduation-rewards-math.spec.ts",
    "test:graduation:math": "playwright test tests/graduation-rewards-math.spec.ts",
    "test:graduation:ui": "playwright test tests/graduation-rewards.spec.ts",
    "test:wallet:connect": "playwright test tests/wallet-connected-token-creation.spec.ts",
    "test:wallet:simulate": "playwright test tests/wallet-connected-token-creation.spec.ts --grep='simulate'",
    "test:wallet:guide": "playwright test tests/wallet-connected-token-creation.spec.ts --grep='guide'",
    "test:hybrid": "playwright test tests/hybrid-token-creation.spec.ts",
    "test:hybrid:manual": "playwright test tests/hybrid-token-creation.spec.ts --grep='manual.*instructions'",
    "test:hybrid:headed": "playwright test tests/hybrid-token-creation.spec.ts --headed",
    "test:semi-auto": "playwright test tests/semi-automated-token-creation.spec.ts --headed --timeout=600000",
    "test:semi-auto:headless": "playwright test tests/semi-automated-token-creation.spec.ts --timeout=600000",
    "test:semi-auto:guide": "playwright test tests/semi-automated-token-creation.spec.ts --grep='guidance'",
    "test:mcp": "playwright test tests/mcp-enhanced-token-creation.spec.ts --headed --timeout=600000",
    "test:mcp:headless": "playwright test tests/mcp-enhanced-token-creation.spec.ts --timeout=600000",
    "test:complete": "playwright test tests/complete-token-graduation.spec.ts --headed --timeout=1500000",
    "test:complete:headless": "playwright test tests/complete-token-graduation.spec.ts --timeout=1500000",
    "test:verify": "playwright test tests/complete-token-graduation.spec.ts --grep='verify' --headed",
    "test:fast": "playwright test tests/fast-token-graduation.spec.ts --headed --timeout=900000",
    "test:fast:headless": "playwright test tests/fast-token-graduation.spec.ts --timeout=900000",
    "test:fast:verify": "playwright test tests/fast-token-graduation.spec.ts --grep='verify' --headed",
    "test:ci": "playwright test --config=playwright-ci.config.ts",
    "test:ci:automated": "playwright test --config=playwright-ci.config.ts --project=ci-automated",
    "test:ci:smoke": "playwright test --config=playwright-ci.config.ts --project=smoke-tests",
    "test:automated": "playwright test tests/automated-token-graduation.spec.ts --timeout=180000",
    "test:automated:headed": "playwright test tests/automated-token-graduation.spec.ts --headed --timeout=180000",
    "test:wallet:automated": "playwright test tests/automated-token-graduation.spec.ts --grep='wallet.*connection'",
    "ci:test": "npm run test:ci",
    "ci:test:full": "npm run test:ci -- --project=ci-automated --project=mathematical-validation --project=feature-validation-automated",
    "test:real": "playwright test tests/real-token-creation.spec.ts --timeout=900000",
    "test:real:headed": "playwright test tests/real-token-creation.spec.ts --headed --timeout=900000",
    "test:real:creation": "playwright test tests/real-token-creation.spec.ts --grep='should create REAL token' --timeout=900000",
    "test:real:verify": "playwright test tests/real-token-creation.spec.ts --grep='should search for and verify' --timeout=120000",
    "test:galachain": "playwright test tests/galachain-wallet-connection.spec.ts --headed --timeout=600000",
    "test:galachain:connect": "playwright test tests/galachain-wallet-connection.spec.ts --grep='should connect to GalaChain wallet' --headed --timeout=600000",
    "test:galachain:balance": "playwright test tests/galachain-wallet-connection.spec.ts --grep='should test GalaChain token balance' --headed --timeout=120000",
    "test:galachain:real": "playwright test tests/galachain-real-token-creation.spec.ts --headed --timeout=900000",
    "test:galachain:create": "playwright test tests/galachain-real-token-creation.spec.ts --grep='should create real token' --headed --timeout=900000",
    "test:galachain:verify": "playwright test tests/galachain-real-token-creation.spec.ts --grep='should verify GalaChain token' --headed --timeout=120000",
    "balance": "node scripts/check-balance.js",
    "balance:testnet": "node scripts/check-balance-testnet-v2.js",
    "wallet:setup": "powershell -ExecutionPolicy Bypass -File setup-wallet.ps1",
    "wallet:balance": "npm run balance",
    "wallet:balance:testnet": "npm run balance:testnet",
    "test:testnet:balance": "playwright test tests/testnet-extension-wallet-balance.spec.ts --headed",
    "test:testnet:auto": "playwright test tests/testnet-auto-wallet.spec.ts --config=playwright-testnet.config.ts",
    "test:testnet": "playwright test --config=playwright-testnet.config.ts",
    "test:sdk": "playwright test tests/sdk-integration.spec.ts --timeout=60000",
    "test:sdk:headed": "playwright test tests/sdk-integration.spec.ts --headed --timeout=60000",
    "test:dex:incentive": "playwright test tests/dex-incentive-apr.spec.ts --config=playwright-dex-test1.config.ts --timeout=90000",
    "test:dex:incentive:headed": "playwright test tests/dex-incentive-apr.spec.ts --config=playwright-dex-test1.config.ts --headed --timeout=90000",
    "test:dex:incentive:api": "playwright test tests/dex-incentive-apr.spec.ts --config=playwright-dex-test1.config.ts --project=dex-incentive-api",
    "test:dex:incentive:ui": "playwright test tests/dex-incentive-apr.spec.ts --config=playwright-dex-test1.config.ts --project=dex-incentive-ui --headed",
    "test:dex:swap": "playwright test tests/dex-swap-transactions.spec.ts --timeout=120000",
    "test:dex:swap:headed": "playwright test tests/dex-swap-transactions.spec.ts --headed --timeout=120000",
    "test:dex:liquidity": "playwright test tests/dex-liquidity-edge-cases.spec.ts --timeout=120000",
    "test:dex:liquidity:headed": "playwright test tests/dex-liquidity-edge-cases.spec.ts --headed --timeout=120000",
    "test:dex:recovery": "playwright test tests/dex-transaction-recovery.spec.ts --timeout=90000",
    "test:dex:recovery:headed": "playwright test tests/dex-transaction-recovery.spec.ts --headed --timeout=90000",
    "test:dex:pools": "playwright test tests/dex-pool-operations.spec.ts --timeout=120000",
    "test:dex:pools:headed": "playwright test tests/dex-pool-operations.spec.ts --headed --timeout=120000",
    "test:dex:all": "playwright test tests/dex-*.spec.ts --timeout=180000",
    "test:dex:all:headed": "playwright test tests/dex-*.spec.ts --headed --timeout=180000",
    "test:dex:comprehensive": "playwright test tests/gala-dex-qa-comprehensive.spec.ts --headed --timeout=300000"
  },
  "dependencies": {
    "@gala-chain/launchpad-sdk": "^5.0.4-beta.18",
    "jira-client": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@tenkeylabs/dappwright": "^2.13.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "keywords": [
    "web3",
    "testing",
    "playwright",
    "dappwright",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
